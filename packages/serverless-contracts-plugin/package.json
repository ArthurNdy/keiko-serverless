{
  "name": "@sls-monorepo/serverless-contracts-plugin",
  "license": "UNLICENSED",
  "version": "1.0.0",
  "private": true,
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "transpile": "babel . --extensions .ts",
    "package": "rm -rf dist && yarn package:cjs && yarn package:esm && yarn package:types",
    "watch": "rm -rf dist && concurrently 'yarn:package:* --watch'",
    "package:cjs": "NODE_ENV=cjs yarn transpile --out-dir dist/cjs --source-maps",
    "package:esm": "NODE_ENV=esm yarn transpile --out-dir dist/esm --source-maps",
    "package:types": "ttsc",
    "test:type": "tsc --noEmit --emitDeclarationOnly false",
    "test:unit": "jest",
    "test:dependents": "lerna run --scope @sls-monorepo/serverless-contracts-plugin --include-dependents --stream --concurrency 2 test:type",
    "package:test": "yarn package && yarn test:dependents",
    "test:all": "yarn test:linter && yarn test:dependents && yarn test:unit",
    "linter-base-config": "eslint --ext=js,ts .",
    "test:linter": "yarn linter-base-config .",
    "lint:fix": "yarn linter-base-config --fix",
    "lint:fix-all": "yarn lint:fix .",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "@sls-monorepo/serverless-contracts": "1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@serverless/typescript": "^2.65.0",
    "@sls-monorepo/configuration": "1.0.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^6.0.0",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "json-schema-to-ts": "^1.6.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "serverless": "^2.66.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "serverless": "^2.60 || 3"
  }
}
