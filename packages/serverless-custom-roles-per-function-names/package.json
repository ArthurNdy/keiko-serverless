{
  "name": "@sls-monorepo/serverless-iam-roles-names-per-function",
  "private": true,
  "version": "1.0.0",
  "license": "UNLICENSED",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "lint:fix": "yarn linter-base-config --fix",
    "lint:fix-all": "yarn lint:fix .",
    "linter-base-config": "eslint --ext=js,ts .",
    "package": "rm -rf dist && yarn package:cjs && yarn package:esm && yarn package:types",
    "package:cjs": "NODE_ENV=cjs yarn transpile --out-dir dist/cjs --source-maps",
    "package:esm": "NODE_ENV=esm yarn transpile --out-dir dist/esm --source-maps",
    "package:test": "yarn package && yarn test:dependents",
    "package:types": "ttsc",
    "precommit": "lint-staged",
    "test:all": "yarn test:linter && yarn test:dependents && yarn test:unit",
    "test:dependents": "lerna run --scope @sls-monorepo/serverless-iam-roles-names-per-function --include-dependents --stream --concurrency 2 test:type",
    "test:linter": "yarn linter-base-config .",
    "test:type": "tsc --noEmit --emitDeclarationOnly false",
    "test:unit": "jest --runInBand --collectCoverage --logHeapUsage",
    "transpile": "babel . --extensions .ts",
    "watch": "rm -rf dist && concurrently 'yarn:package:* --watch'"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@serverless/typescript": "^2.65.0",
    "@sls-monorepo/configuration": "^1.0.0",
    "@types/aws-lambda": "^8.10.85",
    "@types/serverless": "^1.78.39",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "aws-sdk": "^2.1030.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "prettier": "^2.4.1",
    "serverless": "^2.66.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.4"
  }
}
