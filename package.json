{
  "name": "@sls-monorepo/app",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "workspaces": [
    "backend/*",
    "contracts/*",
    "packages/*",
    "frontend/*"
  ],
  "scripts": {
    "check-audit": "check-audit --yarn",
    "deploy": "nx run-many --target=deploy --all",
    "graph": "nx dep-graph",
    "info": "nx run-many --target=sls:info --all",
    "package": "nx run-many --target=package --all",
    "prepare": "husky install && syncpack format",
    "resolve-audit": "resolve-audit --yarn",
    "test:all": "nx run-many --target=test:all --all",
    "test:linter": "nx run-many --target=test:linter --all",
    "test:type": "nx run-many --target=test:type --all",
    "test:unit": "nx run-many --target=test:unit --all",
    "watch": "nx run-many --target=watch --all --parallel --maxParallel 10"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nrwl/cli": "latest",
    "@nrwl/tao": "latest",
    "@nrwl/workspace": "latest",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "aws-sdk": "^2.1047.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lerna": "^4.0.0",
    "lodash": "^4.17.20",
    "npm-audit-resolver": "^2.2.1",
    "prettier": "^2.5.0",
    "syncpack": "^5.6.10",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": "^14.18.1",
    "yarn": "^1.22.15"
  }
}